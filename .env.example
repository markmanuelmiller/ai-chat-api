# Environment Configuration
NODE_ENV=development
PORT=3001
STORAGE_TYPE=memory

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=1d

# Local Logs Configuration
# Path to local directory containing mock binary logs for development
# This directory will be mounted to /var/log/foundation in the Docker container
LOCAL_LOGS_PATH=./mock-logs

# Stream Doctor Configuration
# Mock server URL for development
MOCK_SERVER_URL=http://localhost:3003
